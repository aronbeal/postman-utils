#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/flat@5.0.2/node_modules/flat/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/flat@5.0.2/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/node_modules:/Users/aronbeal/Code/postman-utils/node_modules:/Users/aronbeal/Code/node_modules:/Users/aronbeal/node_modules:/Users/node_modules:/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/yargs-unparser@2.0.0/node_modules/yargs-unparser/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/yargs-unparser@2.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/flat@5.0.2/node_modules/flat/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/flat@5.0.2/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/node_modules:/Users/aronbeal/Code/postman-utils/node_modules:/Users/aronbeal/Code/node_modules:/Users/aronbeal/node_modules:/Users/node_modules:/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/yargs-unparser@2.0.0/node_modules/yargs-unparser/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/yargs-unparser@2.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../flat@5.0.2/node_modules/flat/cli.js" "$@"
else
  exec node  "$basedir/../../../../../flat@5.0.2/node_modules/flat/cli.js" "$@"
fi

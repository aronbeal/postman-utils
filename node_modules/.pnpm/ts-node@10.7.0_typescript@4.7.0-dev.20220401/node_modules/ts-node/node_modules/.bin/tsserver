#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/typescript@4.7.0-dev.20220401/node_modules/typescript/bin/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/typescript@4.7.0-dev.20220401/node_modules/typescript/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/typescript@4.7.0-dev.20220401/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/node_modules:/Users/aronbeal/Code/postman-utils/node_modules:/Users/aronbeal/Code/node_modules:/Users/aronbeal/node_modules:/Users/node_modules:/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_typescript@4.7.0-dev.20220401/node_modules/ts-node/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_typescript@4.7.0-dev.20220401/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/typescript@4.7.0-dev.20220401/node_modules/typescript/bin/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/typescript@4.7.0-dev.20220401/node_modules/typescript/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/typescript@4.7.0-dev.20220401/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/node_modules:/Users/aronbeal/Code/postman-utils/node_modules:/Users/aronbeal/Code/node_modules:/Users/aronbeal/node_modules:/Users/node_modules:/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_typescript@4.7.0-dev.20220401/node_modules/ts-node/node_modules:/Users/aronbeal/Code/postman-utils/node_modules/.pnpm/ts-node@10.7.0_typescript@4.7.0-dev.20220401/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.7.0-dev.20220401/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.7.0-dev.20220401/node_modules/typescript/bin/tsserver" "$@"
fi
